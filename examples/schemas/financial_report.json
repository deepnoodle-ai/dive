{
  "type": "object",
  "title": "Financial Report",
  "description": "Schema for extracting financial data from reports",
  "properties": {
    "company_name": {
      "type": "string",
      "description": "Name of the company"
    },
    "report_period": {
      "type": "object",
      "description": "Reporting period",
      "properties": {
        "start_date": {
          "type": "string",
          "description": "Start date of the reporting period",
          "format": "date"
        },
        "end_date": {
          "type": "string",
          "description": "End date of the reporting period",
          "format": "date"
        }
      }
    },
    "revenue": {
      "type": "number",
      "description": "Total revenue in the reporting period"
    },
    "expenses": {
      "type": "number",
      "description": "Total expenses in the reporting period"
    },
    "net_income": {
      "type": "number",
      "description": "Net income (revenue - expenses)"
    },
    "assets": {
      "type": "object",
      "description": "Company assets",
      "properties": {
        "current_assets": {
          "type": "number",
          "description": "Current assets value"
        },
        "fixed_assets": {
          "type": "number",
          "description": "Fixed assets value"
        },
        "total_assets": {
          "type": "number",
          "description": "Total assets value"
        }
      }
    },
    "liabilities": {
      "type": "object",
      "description": "Company liabilities",
      "properties": {
        "current_liabilities": {
          "type": "number",
          "description": "Current liabilities value"
        },
        "long_term_liabilities": {
          "type": "number",
          "description": "Long-term liabilities value"
        },
        "total_liabilities": {
          "type": "number",
          "description": "Total liabilities value"
        }
      }
    },
    "key_metrics": {
      "type": "object",
      "description": "Key financial metrics",
      "properties": {
        "profit_margin": {
          "type": "number",
          "description": "Profit margin percentage"
        },
        "return_on_assets": {
          "type": "number",
          "description": "Return on assets percentage"
        },
        "debt_to_equity": {
          "type": "number",
          "description": "Debt to equity ratio"
        }
      }
    }
  },
  "required": ["company_name", "revenue", "expenses", "net_income"]
}